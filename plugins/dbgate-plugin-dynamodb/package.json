{
  "name": "dbgate-plugin-dynamodb",
  "main": "dist/backend.js",
  "version": "7.0.0-alpha.1",
  "license": "GPL-3.0",
  "homepage": "https://dbgate.org",
  "description": "DynamoDB connect plugin for DbGate",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbgate/dbgate"
  },
  "keywords": [
    "dbgate",
    "dynamodb",
    "aws",
    "amazon",
    "dbgatebuiltin"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "build:frontend": "webpack --config webpack-frontend.config",
    "build:frontend:watch": "webpack --watch --config webpack-frontend.config",
    "build:backend": "webpack --config webpack-backend.config.js",
    "build": "yarn build:frontend && yarn build:backend",
    "plugin": "yarn build && yarn pack && dbgate-plugin dbgate-plugin-dynamodb",
    "copydist": "yarn build && yarn pack && dbgate-copydist ../dist/dbgate-plugin-dynamodb",
    "plugout": "dbgate-plugout dbgate-plugin-dynamodb",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "dbgate-plugin-tools": "^1.0.7",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.400.0",
    "@aws-sdk/lib-dynamodb": "^3.400.0",
    "@aws-sdk/util-dynamodb": "^3.400.0",
    "dbgate-query-splitter": "^4.11.9",
    "dbgate-sqltree": "^7.0.0-alpha.1",
    "dbgate-tools": "^7.0.0-alpha.1",
    "lodash": "^4.17.21"
  }
}
